#%RAML 1.0 DataType
displayName: Analysis Task Output
type: object
properties:
  id:
    description: Unique identifier for the analysis task
    type: string
    required: true
  reference_id:
    description: Identifier of the existing Paysera resource you want to analyze
    type: string
    required: true
  reference_type:
    description: |
      Type of the provided Paysera resource (`reference_id`) you want to analyze. Possible values are:
      - `information_request`: For [information request](https://gitlab.paysera.net/paysera/api-spec/-/tree/master/app-evpbank/public/transfer-aml-information) resources.
    type: string
    required: true
    enum: ['information_request']
  status:
    description: Current status of the analysis task
    type: string
    enum: ['accepted', 'processing', 'completed']
    required: true
  created_at:
    (datetime_timestamp):
    type: integer
    description: Timestamp of when the analysis task was created
    required: true
